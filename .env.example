# Environment variables for GitHub Scorer
# Copy this file to .env.local and fill in your values

# GitHub Personal Access Token (required)
# Create at: https://github.com/settings/tokens
# Needs 'public_repo' scope for public repositories
GITHUB_TOKEN=your_github_token_here

# MongoDB connection string (optional, for caching)
# If not provided, caching will be disabled
MONGODB_URI=mongodb://localhost:27017/GitScore-Copilot
